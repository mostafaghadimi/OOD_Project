#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/terser/4.7.0/node_modules/terser/bin/terser/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/terser/4.7.0/node_modules/terser/bin/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/terser/4.7.0/node_modules/terser/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/terser/4.7.0/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/terser/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules:/home/mostafa/Desktop/Work/OOD_Project/node_modules:/home/mostafa/Desktop/Work/node_modules:/home/mostafa/Desktop/node_modules:/home/mostafa/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../terser/4.7.0/node_modules/terser/bin/terser" "$@"
else 
  exec node  "$basedir/../../../../../../terser/4.7.0/node_modules/terser/bin/terser" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/rimraf/2.7.1/node_modules/rimraf/bin.js/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/rimraf/2.7.1/node_modules/rimraf/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/rimraf/2.7.1/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/rimraf/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules:/home/mostafa/Desktop/Work/OOD_Project/node_modules:/home/mostafa/Desktop/Work/node_modules:/home/mostafa/Desktop/node_modules:/home/mostafa/node_modules:/home/node_modules:/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules/rimraf/bin.js/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules/rimraf/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else 
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/3.2.1_webpack@4.40.2/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/3.2.1_webpack@4.40.2/node_modules/webpack-dev-server/bin/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/3.2.1_webpack@4.40.2/node_modules/webpack-dev-server/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/3.2.1_webpack@4.40.2/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules:/home/mostafa/Desktop/Work/OOD_Project/node_modules:/home/mostafa/Desktop/Work/node_modules:/home/mostafa/Desktop/node_modules:/home/mostafa/node_modules:/home/node_modules:/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules/webpack-dev-server/bin/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules/webpack-dev-server/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else 
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi

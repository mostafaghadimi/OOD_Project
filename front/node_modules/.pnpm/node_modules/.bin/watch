#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/watch/cli.js/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/watch/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules:/home/mostafa/Desktop/Work/OOD_Project/node_modules:/home/mostafa/Desktop/Work/node_modules:/home/mostafa/Desktop/node_modules:/home/mostafa/node_modules:/home/node_modules:/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules/@cnakazawa/watch/cli.js/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules/@cnakazawa/watch/node_modules:/home/mostafa/Desktop/Work/OOD_Project/front/node_modules/.pnpm/node_modules/@cnakazawa/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cnakazawa/watch/cli.js" "$@"
else 
  exec node  "$basedir/../@cnakazawa/watch/cli.js" "$@"
fi
